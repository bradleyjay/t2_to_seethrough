                "avatarUrls": {
                    "48x48": "https://secure.gravatar.com/avatar/8d0023d4bbdd1905613191dcf0aacf29?d=https%3A%2F%2Favatar-management--avatars.us-west-2.prod.public.atl-paas.net%2Finitials%2FVT-5.png",
